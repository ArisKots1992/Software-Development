package aris.kots.adminclientapplication;


import android.os.Bundle;
import android.support.v4.app.Fragment;
import android.view.LayoutInflater;
import android.view.View;
import android.view.ViewGroup;
import android.widget.TextView;

public class AndreasTab extends Fragment {
  @Override
      public View onCreateView(LayoutInflater inflater, ViewGroup container,
              Bundle savedInstanceState) {
          View android = inflater.inflate(R.layout.third_tab, container, false);
          ((TextView)android.findViewById(R.id.textView)).setText("Andreas");
          
  		      	Activity activity = getActivity();
  		    	Toast.makeText(activity,"MPIKA",Toast.LENGTH_SHORT).show();
  				((TabActivity) getActivity().getParent()).getTabHost().setCurrentTab(1);
          
          return android;
}}
